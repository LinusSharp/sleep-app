{
  "expo": {
    "name": "SlumberLeague",
    "slug": "slumberleague",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0F172A",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linussharp.slumberleague",
      "buildNumber": "21",
      "userInterfaceStyle": "dark",
      "newArchEnabled": false,
      "infoPlist": {
        "NSHealthShareUsageDescription": "SlumberLeague requires access to your sleep data to calculate your recovery score and rank you on the leaderboards.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.healthkit": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "userInterfaceStyle": "dark",
      "package": "com.linussharp.slumberleague"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e87b0838-bd5e-4a25-a5a9-05b71c97215d"
      }
    },
    "plugins": [
      [
        "react-native-health",
        {
          "NSHealthShareUsageDescription": "SlumberLeague requires access to your sleep data to calculate your recovery score and rank you on the leaderboards."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "buildReactNativeFromSource": true
          }
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e87b0838-bd5e-4a25-a5a9-05b71c97215d"
    }
  }
}
